#include <stdio.h>
int main() {
    int arr[]={10,12,17,19,26,100,500,567,26 };
    int find=276;
    int len=sizeof(arr)/sizeof(arr[0]);
    int flag=-1;
    int index =-1;
    for(int i=0;i<len;i++){
        if(find == arr[i]){
            flag=1;
            index=i;
            break;
        }
    }
    if(flag==1){
        printf("Element is found at index %d ",index);
    }else{
        printf("Element is not found ");
    }
    
}
